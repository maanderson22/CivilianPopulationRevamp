//------------------------------------------------FARM PARTS/USI-LS CONVERSION-------------------------------------------------------------------

@PART[stbiodomeFarmMk2]:NEEDS[USILifeSupport]
{
	RESOURCE
	{
		name = Supplies  
		amount = 112000
		maxAmount = 112000
	}
	RESOURCE
	{
		name = Fertilizer 
		amount = 0
		maxAmount = 11200
	}
	RESOURCE
	{
		name = Mulch 
		amount = 0
		maxAmount = 112000
	}

	MODULE
	{
		name = ModuleResourceConverter
		ConverterName = Agroponics
		tag = Agroponics
		StartActionName = Start Agroponics
		StopActionName = Stop Agroponics
		INPUT_RESOURCE
		{
			ResourceName = Mulch
			Ratio =  0.052
		}
		INPUT_RESOURCE
		{
			ResourceName = Fertilizer
			Ratio =  0.0078
		}
		INPUT_RESOURCE
		{
			ResourceName = ElectricCharge
			Ratio =  78
		}
		OUTPUT_RESOURCE
		{
			ResourceName = Supplies
			Ratio = 0.053
			DumpExcess = False
		}
}

